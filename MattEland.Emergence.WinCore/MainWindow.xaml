<Window x:Class="MattEland.Emergence.WinCore.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MattEland.Emergence.WinCore"
        mc:Ignorable="d"
        Title="Emergence Test Bed" Height="450" Width="800" d:DataContext="{d:DesignInstance local:GameViewModel}">
    <Grid>
        <ItemsControl x:Name="WorldControl" ItemsSource="{Binding GameObjects}"></ItemsControl>
    </Grid>
</Window>
